@model WebSite.Models.Entity.TbDocuments
@{
    ViewBag.Title = "UpdateDoc";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<form method="post" class="form-control">
    <label>Açıklama</label>
    @Html.TextBoxFor(x => x.Explanation, new { @class = "form-control" })
    <br />
    <label>Tarih</label>
    @Html.TextBoxFor(x => x.Date, new { @class = "form-control" })
    <br />



    <button class="btn btn-dark">Düzenle</button>
</form>
