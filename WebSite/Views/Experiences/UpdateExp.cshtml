@model WebSite.Models.Entity.TbExperiences
@{
    ViewBag.Title = "UpdateExp";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<form method="post" class="form-control">
    <label>Başlık</label>
    @Html.TextBoxFor(x => x.Title, new { @class = "form-control" })
    <br />
    <label>Altbaşlık</label>
    @Html.TextBoxFor(x => x.Subtitle, new { @class = "form-control" })
    <br />
    <label>Açıklama</label>
    @Html.TextBoxFor(x => x.Explanation, new { @class = "form-control" })
    <br />
    <label>Tarih</label>
    @Html.TextBoxFor(x => x.Date, new { @class = "form-control" })
    <br />


    <button class="btn btn-dark">Güncelle</button>
</form>

