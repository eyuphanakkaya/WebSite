@using WebSite.Models.Entity
@model List<TbContact>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Ad Soyad</th>
        <th>Mail</th>
        <th>Konu</th>
        <th>Mesaj</th>
        <th>Tarih</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.NameSurname</td>
            <td>@item.Mail</td>
            <td>@item.Subject</td>
            <td>@item.Message</td>
            <td>@item.Date</td>
            @*<td><a href="/Experiences/DeleteExp/@item.Id">Sil</a></td>
            <td><a href="/Experiences/UpdateExp/@item.Id">Güncelle</a></td>*@
        </tr>
    }
</table>
